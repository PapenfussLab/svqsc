	methods,
	stats,
	R (>= 3.2.2),
	StructuralVariantAnnotation (>= 0.4.0)
Imports:
	assertthat
Suggests:
	devtools,
	testthat,
	roxygen2,
	covr
RoxygenNote: 5.0.1
